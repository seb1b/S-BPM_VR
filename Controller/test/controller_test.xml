    <Script name_suffix="4" base_name="Script" name_space="__script__" type="Python" mobile="">
      <core>
	import VR
	
	def beispiel1():
		cam = VR.getRoot().find('Default')
	
		path = VR.Path()
		path.set([0,0,20],[0,0,1],[0,1,0], [0,0,30],[0,0,1],[0,1,0], 80)
	
		cam.animate(path, 2, 0, False)
		
	def fly(t):
		cam = VR.getRoot().find('Default')
		p = [0,0,20+t*10]
		cam.setFrom(p)
		
	a = VR.Animation('fly')
	a.setCallback(fly)
	a.setDuration(2)
	
	a.start()
